<hibernate-mapping>
<class name="Register.Signup" table="details">
<id name="userid">
			<generator class="increment"></generator>
		</id>
		<property name="uname"></property>
		<property name="pwd"></property>
		<property name="eid"></property>
</class>
</hibernate-mapping>